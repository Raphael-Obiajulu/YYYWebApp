@{
    ViewData["Title"] = "Login";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!-- ======= Breadcrumbs ======= -->
<div class="breadcrumbs d-flex align-items-center bg-success" style="background-image: url('assets/img/breadcrumbs-bg.jpg');">
    <div class="container position-relative d-flex flex-column align-items-center" data-aos="fade">

        <h2>Login</h2>

    </div>
</div><!-- End Breadcrumbs -->
<!-- ======= Get Started Section ======= -->
<section id="get-started" class="get-started section-bg">
    <div class="container">

        <div class="col-lg-6 m-auto mt-2" data-aos="fade">
            <form class="php-email-form">
                <div class="row gy-3">
                    <div class="mt-4 text-center lead"> <h3> Login to continue </h3></div>

                    <div class="col-md-12 mb-3">
                        <input type="text" class="form-control" id="username" placeholder="username" required>
                    </div>

                    <div class="col-md-12">
                        <input type="password" class="form-control" id="password" placeholder="password" required>
                    </div>

                    <div class="col-md-12 text-center">
                        <button class="btn btn-success" id="submit_btn" style="color: white;" onclick="login()" type="button"> Login </button>
                    </div>

                </div>
            </form>
        </div>

    </div>

</section>
<!-- End Get Started Section -->
